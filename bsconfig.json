{
  "name": "refetch",
  "version": "0.1.0",
  "refmt": 3,
  "bsc-flags": ["-bs-no-version-header", "-bs-super-errors"],
  "package-specs": [{
    "module": "commonjs",
    "in-source": true
  }],
  "suffix": ".bs.js",  
  "sources": [
    "src", {
      "dir": "examples",
      "type": "dev"
    }, {
      "dir": "__tests__",
      "type": "dev"
    }
  ],
  "bs-dependencies" : [
    "bs-fetch",
    "@glennsl/rebase"
  ],
  "bs-dev-dependencies" : [
    "@glennsl/bs-json",
    "@glennsl/bs-jest"
  ]
}
